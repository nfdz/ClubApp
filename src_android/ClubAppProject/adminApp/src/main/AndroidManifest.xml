<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="YOUR.ADMIN.APP.ID.HERE">

    <application
        android:name="io.github.nfdz.clubadmin.ClubAdminApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="io.github.nfdz.clubadmin.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="io.github.nfdz.clubadmin.events.EventsActivity"
            android:label="@string/events_title" />
        <activity android:name="io.github.nfdz.clubadmin.events.editor.EventEditorActivity" />
        <activity
            android:name="io.github.nfdz.clubadmin.events.attendance.EventAttendanceActivity"
            android:label="@string/event_attendance_title"/>
        <activity
            android:name="io.github.nfdz.clubadmin.users.UsersActivity"
            android:label="@string/users_title" />
        <activity android:name="io.github.nfdz.clubadmin.users.editor.UserEditorActivity" />
        <activity
            android:name="io.github.nfdz.clubadmin.alerts.AlertsActivity"
            android:label="@string/alerts_title" />
        <activity
            android:name="io.github.nfdz.clubadmin.qrs.QRsActivity"
            android:label="@string/qrs_title"/>
        <activity
            android:name="io.github.nfdz.clubadmin.chat.ChatActivity"
            android:label="@string/chat_title"/>
    </application>

</manifest>